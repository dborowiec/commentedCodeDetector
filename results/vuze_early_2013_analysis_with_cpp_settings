 			public void selected(MenuItem menu, Object target) {
 				if (target instanceof SideBarEntry) {
 					SideBarEntry info = (SideBarEntry) target;
 					System.err.println(info.getId() + " of " + info.getParentID()
 							+ ";ds=" + info.getDatasource());
 				}
 			}
 		});
