 		idBitMsg msg;
 		if ( ss.GetObjectMsgByID( idSession::SS_PLAYER + i, msg ) ) {
 			if ( msg.GetSize() == 0 ) {
 				snapCurrent.players[ i ].valid = false;
 				continue;
