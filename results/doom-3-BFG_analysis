 * /
 static void PC_PrintDefineHashTable(define_t **definehash) {
 	int i;
 	define_t *d;

 	for (i = 0; i < DEFINEHASHSIZE; i++) {
 		Log_Write("%4d:", i);
 		for (d = definehash[i]; d; d = d->hashnext) {
 			Log_Write(" %s", d->name);
 		}
 		Log_Write("\n");
 	}
